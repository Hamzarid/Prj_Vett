  <body class="u-body u-xl-mode" data-lang="fr">
    <section class="u-align-center u-clearfix u-gradient u-section-1" id="sec-db72">
      <div class="u-clearfix u-sheet u-sheet-1">
        <h1 class="u-custom-font u-font-merriweather u-text u-text-default u-text-1">Vos Informations Personnelles</h1>
        <div class="u-form u-form-1">
          <form method="post" enctype="multipart/form-data" style="padding: 10px">
            <div class="u-form-group u-form-name u-label-top">
              <label for="name-3b9a" style="font-weight: bold;" class="u-label">Nom</label>
             

              <input type="text" id="name-3b9a" name="nom" value="@Model.nom" class="u-border-2 u-border-black u-border-no-left u-border-no-right u-border-no-top u-input u-input-rectangle u-input-1" required="">
            </div>
            <br />

            <div class="u-form-group u-form-name u-label-top u-form-group-2">
             <label  for="name-2e34" style="font-weight: bold;" class="u-label">Prénom</label>
              <input type="text" id="name-2e34" name="prenom" value="@Model.prenom" class="u-border-2 u-border-black u-border-no-left u-border-no-right u-border-no-top u-input u-input-rectangle u-input-2" required="">
            </div>
            <br />
            <div class="u-form-email u-form-group u-label-top">
              <label for="email-3b9a" style="font-weight: bold;" class="u-label">Email</label>
              <input type="email" id="email-3b9a" name="email" value="@Model.email" class="u-border-2 u-border-black u-border-no-left u-border-no-right u-border-no-top u-input u-input-rectangle u-input-3" required="">
            </div>
            <br />
            <div class="u-form-group u-form-phone u-label-top u-form-group-4">
              <label for="phone-c1e9" style="font-weight: bold;"  class="u-label">Téléphone</label>
              <input type="tel" id="phone-c1e9" name="phone" value="@Model.numero" class="u-border-2 u-border-black u-border-no-left u-border-no-right u-border-no-top u-input u-input-rectangle u-input-4" required="">
            </div>
            <br />
            <div class="u-form-address u-form-group u-label-top u-form-group-5">
              <label for="address-1cb8" style="font-weight: bold;" class="u-label">Adresse</label>
              <input type="text" id="address-1cb8" name="address" value="@Model.adress" class="u-border-2 u-border-black u-border-no-left u-border-no-right u-border-no-top u-input u-input-rectangle u-input-5" required="">
            </div>
            <br />
            <div class="u-form-group u-label-top u-form-group-6">
              <label for="text-3da4" style="font-weight: bold;" class="u-label">Réference</label>
              <input type="text" id="text-3da4" name="reference" value="@Model.reference" class="u-border-2 u-border-black u-border-no-left u-border-no-right u-border-no-top u-input u-input-rectangle u-input-6">
            </div>
            <br />
             <div class="u-form-group u-label-top u-form-group-6">
              <label for="text-3da4" style="font-weight: bold;" class="u-label">Nom d'utilisateur</label>
              <input type="text" id="text-3da4" name="user" value="@Model.user" class="u-border-2 u-border-black u-border-no-left u-border-no-right u-border-no-top u-input u-input-rectangle u-input-6">
            </div>
            <br />
             <div class="u-form-group u-label-top u-form-group-6">
              <label for="text-3da4" style="font-weight: bold;" class="u-label">Mot de passe</label>
              <input type="password" id="text-3da4" name="pwd" value="@Model.pwd" class="u-border-2 u-border-black u-border-no-left u-border-no-right u-border-no-top u-input u-input-rectangle u-input-6">
            </div>
            <br />
           
                <div class="u-align-center u-form-group  ">
                <input type="submit" name="Modifier"class="u-border-2 u-border-black u-btn u-btn-round u-btn-submit u-button-style u-none u-radius-30 u-btn-1" value="Modifier mes informations"
                asp-page-handler="Soumettre"  />
                </div>
            <br />
             @Html.Raw(Model.alert)
            
          </form>
        </div>
      </div>
    </section>
    <script>
var close = document.getElementsByClassName("closebtn");
var i;

for (i = 0; i < close.length; i++) {
  close[i].onclick = function(){
    var div = this.parentElement;
    div.style.opacity = "0";
    setTimeout(function(){ div.style.display = "none"; }, 600);
  }
}
</script>
</body></html>
